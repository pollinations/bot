{
  "name": "@pollinations/bot",
  "version": "0.1.0",
  "description": "Pollinations' Discord Bot",
  "scripts": {
    "start": "npm run build && node -r dotenv/config dist/discord/index.js dotenv_config_path=.env.production.local",
    "dev": "NODE_ENV=development nodemon -r dotenv/config src/discord/index.ts dotenv_config_path=.env.local",
    "build": "tsc",
    "deploy-commands": "ts-node --experimental-specifier-resolution=node -r dotenv/config src/discord/deployCommands.ts dotenv_config_path=.env.production.local",
    "deploy-commands-dev": "ts-node --experimental-specifier-resolution=node -r dotenv/config src/discord/deployCommands.ts dotenv_config_path=.env.local",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --config .prettierrc --write src/**/*.ts",
    "lint": "eslint src/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "@pollinations/ipfs": "github:pollinations/pollinations-ipfs",
    "app-root-path": "^3.1.0",
    "await-sleep": "^0.0.1",
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "npm": "^8.15.0",
    "readline-sync": "^1.4.10",
    "social-post-api": "^1.0.20",
    "twitter-api-sdk": "^1.1.0",
    "twitter-api-v2": "^1.12.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
